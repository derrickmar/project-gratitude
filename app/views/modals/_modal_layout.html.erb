<div class="modal fade intro-modal" id="<%= modalID %>" tabindex="-1" role="dialog" aria-labelledby="<%= modalID %>" aria-hidden="true">
  <div class="modal-dialog modal-lg">
     <%= image_tag "pin.png", class: "pin-show-note" %>
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">
          <span class="x-button" aria-hidden="true">&times;</span>
          <span class="sr-only">Close</span>
        </button>
      	<%= yield_content! :modal_header %>
      </div>
      <%= yield_content! :above_modal_body %>
      <div id="<%= modalBodyID %>" class="modal-body row">
      	<%= yield_content! :modal_body %>
      </div>
    </div>
  </div>
</div>